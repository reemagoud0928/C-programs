#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,i,sum=0;
    scanf("%d%d",&a,&b);
    for(i=a;i<=b;i++)
        sum=sum+i;
    {
        printf("%d",sum);
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
