#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 int a,w; 
    float h; 
    scanf("%d%d%f",&a,&w,&h);
    if(a>=18&&w>=45&&h>=5.5)
    { 
        printf("He or She can donate blood"); 
    }
    else {
        printf("He or She unable to donate blood");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
