#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    int longs=(a*b)/100;
    int rem=a-longs;
    int beard=(rem*c)/100;
    int others=(rem-beard)/3;
    printf("%d\n%d\n%d\n",longs,beard,others);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
