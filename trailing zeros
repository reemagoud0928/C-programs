#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
   int fact=1;
    int count=0;
    for(int i=1;i<=n;i++)
    {
        fact=fact*i;
    }
while(fact!=0){
    int r=fact%10;
    fact=fact/10;
    if(r==0)
    {
        count++;
        
    }
    else{
        break;
    }
}
    printf("%d",count);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
