#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,sum=0;
    scanf("%d",&a);
    while(a!=0)
    {
        int i=a%10;
        a=a/10;
        if(i%2!=0)
            sum=sum+i;
    }
    printf("%d",sum);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
